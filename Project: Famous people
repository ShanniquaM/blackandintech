    VALUES (10, "Actor");
    
SELECT famouspeople.first_name, famouspeople.last_name, famouspeople_work.occupation
FROM famouspeople
JOIN famouspeople_work
ON famouspeople.id = famouspeople_work.famouspeople_id;
SELECT * FROM famouspeople;
